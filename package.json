{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": "14.16.x",
    "npm": "6.14.x"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "start": "webpack-dev-server",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --env.prod=true",
    "postinstall": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --env.prod=true --mode=production",
    "server": "node server"
  },
  "author": "Dat Tran",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "3.1.4",
    "css-loader": "^2.1.1",
    "enzyme": "^2.9.1",
    "file-loader": "3.0.1",
    "jest": "^24.9.0",
    "jest-enzyme": "^3.4.0",
    "node-cache": "^4.2.1",
    "node-sass": "^4.5.0",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "react": "^16.14.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^16.14.0",
    "react-fontawesome": "^1.7.1",
    "react-icons": "^2.2.5",
    "react-test-renderer": "^16.14.0",
    "sass-loader": "^6.0.1",
    "style-loader": "0.13.1",
    "svg-react-loader": "^0.4.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      ".*\\.(css|scss|sass)$": "<rootDir>/internals/cssMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/imageMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "webpack": "^4.46.0"
  }
}
